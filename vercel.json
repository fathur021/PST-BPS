{
  "version": 3,
  "framework": "laravel",
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  }
}